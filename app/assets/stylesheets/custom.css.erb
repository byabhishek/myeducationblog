.front-bg {
  background-image: url(<%= asset_path 'front.jpg' %>);
  background-color:#4D8D6E;
  background-size: cover;
  margin-top:0px;
  background-repeat: no-repeat;
  height:100%;
  width:100%;
  }

  .back-bg {
    background-image: url(<%= asset_path 'wbg.jpg' %>);
    background-color:#4D8D6E;
    background-size: cover;
    margin-top:0px;
    background-repeat: no-repeat;
    height:100%;
    width:100%;
    }

  .ripple {
    height: 35vh;
    width: auto;
}

.ripple img {
    object-fit: cove;
    object-position: top;
    display: block;
    height: 100%;
    width: 100%;
}

